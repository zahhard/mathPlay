<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_gragh"
    app:startDestination="@id/gameSettingFragment">
    <fragment
        android:id="@+id/gameSettingFragment"
        android:name="com.example.mathplay.GameSettingFragment"
        android:label="GameSettingFragment" >
        <action
            android:id="@+id/action_gameSettingFragment_to_gameFragment"
            app:destination="@id/gameFragment" />
    </fragment>
    <fragment
        android:id="@+id/gameFragment"
        android:name="com.example.mathplay.GameFragment"
        android:label="GameFragment" >
        <action
            android:id="@+id/action_gameFragment_to_resultFragment"
            app:destination="@id/resultFragment" />
        <action
            android:id="@+id/action_gameFragment_to_gameSettingFragment"
            app:destination="@id/gameSettingFragment" />
    </fragment>
    <fragment
        android:id="@+id/resultFragment"
        android:name="com.example.mathplay.ResultFragment"
        android:label="ResultFragment" >
        <action
            android:id="@+id/action_resultFragment_to_gameFragment"
            app:destination="@id/gameFragment" />
    </fragment>
</navigation>